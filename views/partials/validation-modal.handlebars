<!-- MODAL - BOOTSTRAP -->
<section
  class='modal fade'
  id='no-input-model'
  tabindex='-1'
  aria-labelledby='No input alert'
  aria-hidden='true'
>
  <div class='modal-dialog'>
    <div class='modal-content'>
      <div class='modal-header'>
        <h5 class='modal-title' id='no-input-title'></h5>
        <button
          type='button'
          class='close'
          data-dismiss='modal'
          aria-label='Close'
        >
          <span aria-hidden='true'>&times;</span>
        </button>
      </div>
      <div class='modal-body' id='no-input-body'></div>
      <div class='modal-footer'>
        <button type='button' class='btn btn-secondary' data-dismiss='modal'>
          Close
        </button>
      </div>
    </div>
  </div>
</section>